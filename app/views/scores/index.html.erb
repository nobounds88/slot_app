<p id="notice"><%= notice %></p>

<h1>Scores</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Store</th>
      <th>Model</th>
      <th>Seat</th>
      <th>Start at</th>
      <th>End at</th>
      <th>Investment</th>
      <th>Proceeds</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.user_id %></td>
        <td><%= score.store_id %></td>
        <td><%= score.model %></td>
        <td><%= score.seat %></td>
        <td><%= score.start_at %></td>
        <td><%= score.end_at %></td>
        <td><%= score.investment %></td>
        <td><%= score.proceeds %></td>
        <td><%= link_to 'Show', score %></td>
        <td><%= link_to 'Edit', edit_score_path(score) %></td>
        <td><%= link_to 'Destroy', score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Score', new_score_path %>
